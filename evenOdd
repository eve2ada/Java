public static void evenOdd(int[] input){
	int temp = 0;
	for(int i = 0; i < input.length; i ++){
	        if(i%2 != input[i]%2){
	            for(int j = i + 1; j < input.length; j ++){
	                if(j%2 != input[j]%2){
	                    if(i%2 != j%2){
	                       temp = input[i];
	                       input[i] = input[j];
	                       input[j] = temp;
	                    }
	                }
	            }
                }
 
	}
}
